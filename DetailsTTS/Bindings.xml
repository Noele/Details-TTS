<!-- DetailsTTS Key Bindings -->
<Bindings>
    <Binding name="DETAILSTTSMENU" default="INSERT" category="Details-TTS">
        DetailsTTSMenu();
    </Binding>
    <Binding name="DETAILSTTSUP"  default = "PAGEUP" category="Details-TTS">
        DetailsTTSUp();
    </Binding>
	<Binding name="DETAILSTTSDOWN"  default = "PAGEDOWN" category="Details-TTS">
        DetailsTTSDown();
